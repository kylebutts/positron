{
  "name": "positron-ipywidgets",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.0.1",
  "publisher": "vscode",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Data Science",
    "Notebooks",
    "Visualization"
  ],
  "keywords": [
    "jupyter",
    "ipywidgets",
    "python"
  ],
  "enabledApiProposals": [
    "contribNotebookStaticPreloads"
  ],
  "contributes": {
    "notebookPreload": [
      {
        "type": "jupyter-notebook",
        "localResourceRoots": [],
        "entrypoint": "./preload-out/index.js"
      }
    ],
    "notebookRenderer": [
      {
        "id": "positron-ipywidgets",
        "displayName": "Positron IPyWidgets",
        "mimeTypes": [
          "application/vnd.jupyter.widget-view+json"
        ],
        "entrypoint": "./renderer-out/index.js",
        "requiresMessaging": "always"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn build-preload && yarn build-renderer",
    "build-preload": "node ./esbuild-preload",
    "build-renderer": "node ./esbuild-renderer"
  },
  "dependencies": {
    "@jupyter-widgets/base-manager": "^1.0.8",
    "@jupyter-widgets/html-manager": "^1.0.10",
    "@lumino/coreutils": "^2.1.2",
    "@lumino/widgets": "^2.3.2"
  },
  "devDependencies": {
    "@types/vscode-webview": "^1.57.5",
    "typescript": "^5.4.5"
  }
}
